{
  "mel": {
    "name": "Mass Equipment List(MEL)",
    "diagrams": {
      "all-rows": "mel-diagram"
    },
    "columnNames": {
        "c1_id": "Id",
        "c1_name": "Canonical Name",
        "m" : "mass"
    },
    "queries": {
      "subsystem": "mel_system.sparql",
      "assembly": "mel.sparql"
    },
    "rowMapping": {
        "id": "subsystem",
        "name": "System",
        "labelFormat": "{subsystem}",
        "subRowMappings":[
          {
            "id": "assembly",
            "name": "Assembly",
            "labelFormat": "{assembly}"
          }
        ]
    }
  }
}
